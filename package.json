{
  "name": "react-template-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "availity-reactstrap-validation": "2.0.2",
    "bootswatch": "^4.1.1",
    "commitizen": "^4.0.4",
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "reactstrap": "5.0.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:changes": "react-scripts test --coverage --onlyChanged --watchAll=false ",
    "eject": "react-scripts test --env=jsdom && react-scripts eject ",
    "lint": "eslint ./src",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --tab-width=4 --write \"src/**/*.{js,jsx,json,css}\"",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --tab-width=4 --write",
      "yarn lint:fix",
      "yarn lint:css",
      "git add ."
    ]
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "lint-staged": "7.0.4",
    "prettier": "1.11.1",
    "source-map-explorer": "1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh scripts/validate-branch-name.sh && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-push": "TZ=Europe/London yarn test:changes"
    }
  },
  "config": {
    "commitizen": {
      "path": "scripts/commit-msg"
    }
  }
}
